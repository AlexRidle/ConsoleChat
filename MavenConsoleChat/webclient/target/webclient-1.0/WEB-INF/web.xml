<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>webclient</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>StartForm.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
        <servlet-name>WebClient</servlet-name>
        <servlet-class>org.maksim.chakur.servlet.WebClient</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>WebClient</servlet-name>
        <url-pattern>/org.maksim.chakur.client.Client</url-pattern>
    </servlet-mapping>
    
</web-app>